<div class="half-width-card-center">
  <div class="information-container">
    <h3><%= title %></h3>
    <p><%= content %></p>
  </div>
  <div class="picture-container">
  <% pictures.each do |picture| %>
    <div class="picture-wrapper">
      <%= image_tag picture.avatar.url.to_s %>
    </div>
  <% end %>
</div>